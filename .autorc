{
	"release": {
		"prerelease": true
	},
	"plugins": [
		"git-tag",
		"released",
		[
			"conventional-commits",
			{
				"defaultReleaseType": "patch"
			}
		],
		[
			"omit-commits",
			{
				"subject": [
					"WIP"
				],
				"labels": [
					"WIP"
				]
			}
		]
	],
	"labels": [
		{
			"name": "major",
			"changelogTitle": "ðŸ‘” Key changes",
			"releaseType": "major",
			"color": "red",
			"overwrite": true
		},
		{
			"name": "minor",
			"changelogTitle": ":sparkles: Supplementary",
			"releaseType": "minor",
			"color": "orange",
			"overwrite": true
		},
		{
			"name": "patch",
			"changelogTitle": ":wrench: Improvement",
			"releaseType": "patch",
			"color": "blue",
			"overwrite": true
		},
		{
			"name": "trivial",
			"releaseType": "skip",
			"overwrite": true,
			"color": "gray"
		},
		{
			"name": "WIP",
			"releaseType": "skip",
			"overwrite": true,
			"color": "gray"
		}
	],
	"owner": "oskar-gmerek",
	"repo": "oskargmerek.com",
	"name": "Oskar Gmerek",
	"email": "oskarg90@gmail.com"
}
